<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<title>CSS Dock Menu</title>
<script type="text/javascript" src="js/jquery.js"></script>
<script type="text/javascript" src="js/interface.js"></script>

<link href="assets/style.css" rel="stylesheet" type="text/css" />

<!--[if lt IE 7]>
 <style type="text/css">
 .dock img { behavior: url(iepngfix.htc) }
 </style>
<![endif]-->

</head>
<body>
<!--top dock -->
<div class="dock" id="dock">
  <div class="dock-container">
	  <a class="dock-item" href="#"><img src="assets/home.png" alt="Inicio" /><span>Inicio</span></a> 
	  <a class="dock-item" href="#"><img src="assets/email.png" alt="Contacto" /><span>Contacto</span></a> 
	  <a class="dock-item" href="#"><img src="assets/history.png" alt="history" /><span>Historia</span></a> 
	  <a class="dock-item" href="#"><img src="assets/calendar.png" alt="calendar" /><span>Calendario</span></a> 
	  <a class="dock-item" href="#"><img src="assets/rss.png" alt="rss" /><span>Mi cuenta</span></a>
	  <a class="dock-item" href="#"><img src="assets/rss.png" alt="rss" /><span>Salir</span></a> 
	  
  </div> 
</div>

<!--bottom dock -->


<!--dock menu JS options -->
<script type="text/javascript">
	
	$(document).ready(
		function()
		{
			$('#dock').Fisheye(
				{
					maxWidth: 80,
					items: 'a',
					itemsText: 'span',
					container: '.dock-container',
					itemWidth: 60,
					proximity: 30,
					halign : 'center'
				}
			)
		}
	);

</script>
</body>
</html>
